<%= link_to "Home", root_path, class: 'brand' %>
<ul class="nav">
    <% if current_user %>
        <%= link_to 'Sign Out', logout_path, 
             method: :delete, 
             confirmation: "Are you sure" 
        %>
    <% else %>
        <%= link_to "Log In", login_path %> or &nbsp;
        <%= link_to "Sign Up", signup_path %>
    <% end %>
</ul>